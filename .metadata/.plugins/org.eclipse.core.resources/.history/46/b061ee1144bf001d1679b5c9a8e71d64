package ajedrezConClases;

public class Casilla {

	//Atributos
	private int fila;
	private int columna;
	
	//Constructor
	public Casilla(int fila, int columna) {
		
		this.fila=fila;
		this.columna=columna;
	}

	public int getFila() {
		return fila;
	}

	public int getColumna() {
		return columna;
	}
	
	
}
